{
  "version": "0.2.0",
  "configurations": [
    // For these launch configurations to work, you need to setup a GODOT4
    // environment variable. On mac or linux, this can be done by adding
    // the following to your .zshrc, .bashrc, or .bash_profile file:
    // export GODOT4="/Applications/Godot.app/Contents/MacOS/Godot"
    {
      "name": "Launch",
      "type": "coreclr",
      "request": "launch",
      "preLaunchTask": "build",
      "program": "/home/avnzx/Desktop/projects/godot/AZ-1/Server/godot/bin/godot.linuxbsd.editor.double.x86_64.mono",
      "args": [
        "--headless"
      ],
      "console": "internalConsole",
      "cwd": "${workspaceFolder}",
      "stopAtEntry": false,
    },
    {
      "name": "Attach",
      "type": "coreclr",
      "request": "attach",
      "processId": "${command:pickProcess}"
    }
  ]
}